{
	"name": "mermetbt/biome",
	"description": "Biome Framework",
	"type": "library",
	"require": {
		"php": ">=5.6.0",
		"psr/log": "^1.0@dev",
		"koala-framework/composer-extra-assets": "^1.1@dev",
		"league/route": "1.2.3",
		"filp/whoops": "2.1.2",
		"sabre/xml": "1.4.1",
		"symfony/console": "3.0.6"
	},
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Timoth√© Mermet-Buffet",
			"email": "mermetbt@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"autoload": {
		"psr-0": {
			"Biome": "src/"
		},
		"classmap": []
	},
	"extra": {
		"require-bower": {
			"jquery": "2.*",
			"bootstrap": "3.*",
			"font-awesome": "4.7.*",
			"datatables": "1.10.*",
			"datatables-plugins": "1.10.*",
			"datatables-responsive-helper": "*",
			"select2-ng": "4.0.*",
			"autosize": "3.0.*",
			"jquery.easy-pie-chart": "2.1.*"
		},
		"require-npm": {
			"gulp": "3.9.*",
			"gulp-rimraf": "0.2.*",
			"gulp-coffee": "2.3.*",
			"gulp-less": "3.0.*",
			"gulp-sass": "2.1.*",
			"gulp-concat": "2.6.*",
			"gulp-concat-css": "2.2.*",
			"gulp-cssmin": "0.1.*",
			"gulp-postcss": "6.0.*",
			"gulp-rucksack": "0.1.*",
			"gulp-rename": "1.2.*",
			"gulp-uglify": "1.5.*",
			"gulp-util": "3.0.*",
			"gulp-chug": "*",
			"precss": "1.4.*",
			"rucksack-css": "0.8.*"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"gulp build"
		],
		"post-update-cmd": [
			"gulp build"
		]
